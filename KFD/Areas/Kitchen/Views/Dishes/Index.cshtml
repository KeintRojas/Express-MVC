@*@model List<Dish>*@

<partial name="_Notifications" />
<div class="container">
	<div class="row pt-4">
		<div class="col-6">
			<h2>Lista de Platos</h2>
		</div>

		<div class="col-6 text-end">
			<a asp-controller="Dishes" asp-action="Create" class="btn btn-primary">
				<i class="bi bi-plus-circle"></i>Nuevo Plato
			</a>
		</div>
	</div>
	<table class="table table-striped table-bordered" id="tblData">
		<thead>
			<tr>
				<th>Name</th>
				<th>Description</th>
				<th>Price</th>
				<th>IsEnable</th>
				<th>Picture</th>
				<th>Acciones</th> @*Actions*@
			</tr>
		</thead>
	</table>
</div>


<script src="~/js/dishKitchen.js"></script>
